<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffefe"
    android:orientation="vertical"
    tools:context=".Views.ProfileFragment">

        <LinearLayout
            android:id="@+id/linearLayout_documents"
            android:layout_width="223dp"
            android:layout_height="173dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="0"
            android:background="#eeeeee"
            android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView_doc"
                    android:layout_width="114dp"
                    android:layout_height="87dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:layout_weight="0"
                    tools:src="@tools:sample/avatars" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_weight="0"
                    android:background="#6e6e6f"
                    android:text="   Justificatif.png"
                    android:textColor="#e9e8e9"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="34dp"
                    android:layout_weight="0"
                    android:background="#6e6e6f"
                    android:text="     Nov 09,2022"
                    android:textColor="#e9e8e9"
                    android:textSize="9sp" />
        </LinearLayout>

</LinearLayout>
